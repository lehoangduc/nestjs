{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "dev": "env-cmd -f .env.development nest start --watch",
    "build": "nest build",
    "pm2:start": "env-cmd -f .env.production pm2 start dist/main.js --name api",
    "pm2:restart": "env-cmd -f .env.production pm2 restart api --update-env",
    "pm2:start:staging": "env-cmd -f .env.staging pm2 start dist/main.js --name api-staging",
    "pm2:restart:staging": "env-cmd -f .env.staging pm2 restart api-staging --update-env",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:dev": "nest start test --watch",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/ar-api/test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.186.0",
    "@aws-sdk/cloudfront-signer": "^3.186.0",
    "@aws-sdk/s3-presigned-post": "^3.186.0",
    "@aws-sdk/s3-request-presigner": "^3.186.0",
    "@casl/ability": "^6.3.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.1.4",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.4",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-fastify": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^1.1.2",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "handlebars": "^4.7.7",
    "ioredis": "^5.2.3",
    "lodash": "^4.17.21",
    "mongodb": "^4.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nestjs-console": "^8.0.0",
    "nestjs-i18n": "^9.2.0",
    "nestjs-pino": "^3.1.1",
    "nestjs-typeorm-paginate": "^4.0.2",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino-http": "^8.2.1",
    "pino-pretty": "^9.1.1",
    "qs": "^6.11.0",
    "randomstring": "^1.2.2",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.4",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.1.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.8.3",
    "@types/nodemailer": "^6.4.6",
    "@types/ratelimiter": "^3.4.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.1.2",
    "plugin": "^0.3.3",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "yarn-upgrade-all": "^0.7.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
